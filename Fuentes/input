inicio
//Variables sencillas.
variableConstante := 5;
variableConExpresion := 1 + 2;
variableParentesis := (1 - 3);
variableParentesisCompleja := 5 + (1 - 2) + (6-1);
variableConOtrasVariables := variableConstante + variableConExpresion;
variableParentesisVariables := (variableConstante - variableParentesis) + variableParentesisCompleja;

//Variables que utilizan operadores multiplicativos.
variableMultiplicacion := ((3*2) + 3) * 10;
variableDivision := (((3/3) - 5) /1) / 10;
prueba := 5 + 3 * 5;

//Prueba sencilla de leer y escribir.
leer(a);
escribir(b);

//Prueba mas completa de leer y escribir.
leer(variableConstante, a);
escribir(variableParentesis, 5, (1-1)); //Comentario en linea.

fin
